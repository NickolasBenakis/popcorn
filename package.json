		"react-icons": "^3.7.0",
		"react-redux": "^7.0.3",
		"react-router": "^5.0.1",
		"react-router-dom": "^5.0.1",
		"react-test-renderer": "^16.8.6",
		"react-tooltip": "^3.11.1",
		"react-use-form-state": "^0.11.0",
		"redux": "^4.0.1",
		"redux-logger": "^3.0.6",
		"redux-mock-store": "^1.5.3",
		"redux-thunk": "^2.3.0",
		"tachyons": "^4.11.1",
		"typescript": "^3.3.4000"
	}
