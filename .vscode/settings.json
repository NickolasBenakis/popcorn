{
    "eslint.enable": true,
    "editor.formatOnSave": true,
    "eslint.autoFixOnSave": true,
    "eslint.alwaysShowStatus": true,
    "indentRainbow.indentSetter": {
        "javascript": {
            "tabSize": 4,
            "insertSpaces": true
        },
        "json": {
            "tabSize": 4,
            "insertSpaces": true
        }
    },
    "javascript.autoClosingTags": false,
    "javascript.format.enable": true,
    "json.format.enable": true
}